{
  "name": "angular-es6-starter",
  "version": "0.0.0",
  "description": "An Angular Webpack Starter kit featuring Angular (Router, Http, Forms, Services, Tests, E2E, Coverage), Karma, Protractor, Jasmine, Istanbul, ES6, and Webpack by Pixels and Code inspired by AngularClass angular-starter",
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "webpack",
    "es6",
    "babel"
  ],
  "author": "Amiralisobhgol <amiralisobhgol@gmail.com>",
  "homepage": "https://github.com/AngularClass/angular-starter",
  "license": "MIT",
  "scripts": {
    "build:dev": "yarn run clean:dist && yarn run webpack -- --config config/webpack/webpack.dev.js --progress --profile",
    "build:docker": "yarn run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "yarn run clean:dist && yarn run webpack -- --config config/webpack/webpack.prod.js  --progress --profile --bail",
    "build": "yarn run build:dev",
    "ci:jit": "yarn run lint && yarn run test && yarn run build:prod && yarn run e2e",
    "ci:nobuild": "yarn run lint && yarn test && yarn run e2e",
    "ci:testall": "yarn run lint && yarn run test && yarn run build:prod && yarn run e2e",
    "ci:travis": "yarn run lint && yarn run test && yarn run build:prod && yarn run e2e:travis",
    "lint": "eslint src/",
    "ci": "yarn run ci:testall",
    "clean:dll": "yarn run rimraf -- dll",
    "clean:dist": "yarn run rimraf -- dist",
    "clean:install": "yarn set progress=false && yarn install",
    "clean": "yarn cache clean --force && yarn run rimraf -- node_modules doc coverage dist compiled dll",
    "docker": "docker",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "github-deploy:dev": "yarn run webpack -- --config config/webpack/webpack.github-deploy.js --progress --profile --env.githubDev",
    "github-deploy:prod": "yarn run webpack -- --config config/webpack/webpack.github-deploy.js --progress --profile --env.githubProd",
    "github-deploy": "yarn run github-deploy:dev",
    "node": "node",
    "postinstall": "yarn run webdriver:update",
    "postversion": "git push && git push --tags",
    "preclean:install": "yarn run clean",
    "preversion": "yarn test",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && yarn run protractor",
    "protractor:live": "protractor --elementExplorer",
    "rimraf": "rimraf",
    "server:dev:hmr": "yarn run server:dev -- --inline --hot",
    "server:dev": "yarn run webpack-dev-server -- --config config/webpack/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "server": "yarn run server:dev",
    "start:hmr": "yarn run server:dev:hmr",
    "start": "yarn run server:dev",
    "test": "yarn run lint && karma start",
    "version": "yarn run build",
    "watch:dev:hmr": "yarn run watch:dev -- --hot",
    "watch:dev": "yarn run build:dev -- --watch",
    "watch:prod": "yarn run build:prod -- --watch",
    "watch:test": "yarn run test -- --auto-watch --no-single-run",
    "watch": "yarn run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "yarn run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
    "@angular/animations": "~4.1.2",
    "@angular/common": "~4.1.2",
    "@angular/compiler": "~4.1.2",
    "@angular/core": "~4.1.2",
    "@angular/forms": "~4.1.2",
    "@angular/http": "~4.1.2",
    "@angular/platform-browser": "~4.1.2",
    "@angular/platform-browser-dynamic": "~4.1.2",
    "@angular/platform-server": "~4.1.2",
    "@angular/router": "~4.1.2",
    "@angularclass/hmr": "~1.2.2",
    "@angularclass/hmr-loader": "~3.0.2",
    "angular2-jwt": "^0.2.3",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "http-server": "^0.9.0",
    "ie-shim": "^0.1.0",
    "ng2-slim-loading-bar": "^4.0.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "~5.0.2",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~4.0.3",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular2-template-loader": "^0.6.2",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-angular2": "0.0.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^4.1.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "^0.11.1",
    "find-root": "^1.0.0",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "~2.0.3",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "~2.0.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "optimize-js-plugin": "0.0.4",
    "parse5": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "preload-webpack-plugin": "^1.2.2",
    "protractor": "^5.1.1",
    "pug": "^2.0.0-rc.2",
    "pug-html-loader": "^1.1.5",
    "raw-loader": "0.5.1",
    "rimraf": "~2.6.1",
    "sass-loader": "^6.0.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "source-map-loader": "^0.2.1",
    "string-replace-loader": "~1.2.0",
    "style-loader": "^0.18.1",
    "to-string-loader": "^1.1.5",
    "url-loader": "^0.5.8",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "~2.4.2",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "~4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AngularClass/angular-starter.git"
  },
  "bugs": {
    "url": "https://github.com/AngularClass/angular-starter/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "yarn": ">= 3"
  }
}
